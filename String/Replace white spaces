void replaceWhiteSpaces() {
	String str = "       Geeks     for    Geeks   " ;
	str = str.replaceAll(" ", "");
	System.out.println(str);
}
----------------------------------
void replaceWhiteSpaces() {
	String str = "       Geeks     for    Geeks   " ;
	StringBuffer sb = new StringBuffer();
	for (int i = 0; i < str.length(); i++) {
		if(str.charAt(i) != ' ') {
			sb.append(str.charAt(i));
		}
	}
	System.out.println(sb);
}
