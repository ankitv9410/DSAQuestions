class Solution {
	static class Node {
		int data;
		Node next;
	}

	static class LinkedList {
		Node head;
		Node tail;
		int size;

		public void addLast(int val) {
			Node temp = new Node();
			temp.data = val;
			temp.next = null;

			if (size == 0) {
				head = tail = temp;
			} else {
				tail.next = temp;
				tail = temp;
			}
			size++;
		}

		public int llSize() {
			return size;
		}
		public int print() {
			Node temp = head;
			while(temp.next!=null) {
				System.out.print(temp.data+" ");
				temp = temp.next;
			}
			return 0;
		}
	}

	public static void main(String args[]) {
		LinkedList ll = new LinkedList();
		for (int i = 0; i < 5; i++) {
			ll.addLast(10 + i);
		}
		System.out.println(ll.llSize());
	}
}
