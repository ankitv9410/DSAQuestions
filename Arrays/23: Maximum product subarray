class Solution {
	public static void maxSubarray(int []arr) {
		
		int max = 0;
		for (int i = 0; i < arr.length; i++) {
			int mult = 1;
			for (int j = i; j < arr.length; j++) {
				mult *= arr[j]; 
				if(mult > max)
					max = mult;
			}
		}
		System.out.println(max);
	}
	public static void main(String args[]) {
		int []arr = {-2, -40, 0, -2, -3};
		maxSubarray(arr);
	}
}                         time O(n^2) space O(1)
-------------------------------------------------
