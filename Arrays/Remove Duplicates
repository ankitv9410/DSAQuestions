public void count(int []arr) {
		int i = 0;
        for (int j = 1; j < arr.length; j++) {
            if (arr[i] != arr[j]) {
                i++;
                arr[i] = arr[j];
            }
            for(int k : arr)
    			System.out.print(k+" ");
            System.out.println();
        }
		for(int k : arr)
			System.out.print(k+" ");
	}
