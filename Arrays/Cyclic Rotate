class Solution {
	public static void rotate(int[] arr) {
		int n = arr.length-1;
		for (int i = 0; i < 1; i++) {
			int temp = arr[n];
			for (int j = n; j > 0; j--) 
				arr[j] = arr[j-1];
			arr[0] = temp;
		}
	}
	public static void main(String args[]) {
		int[] arr = { 1, 2, 3, 4, 5 };
		rotate(arr);
		for (int i : arr) {
			System.out.println(i);
		}
	}
}
